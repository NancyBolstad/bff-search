    "eslint-loader": "^3.0.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-no-for-each": "^0.1.14",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.5"
  }
