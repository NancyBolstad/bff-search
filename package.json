{
  "name": "@entur/entur-search",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node .",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "dev": "npm run build:dev && nodemon .",
    "staging": "npm run build:staging && nodemon .",
    "prod": "npm run build:prod && nodemon .",
    "prebuild": "npm run lint",
    "build:dev": "tsc && npm run populate-env-vars dev",
    "build:staging": "tsc && npm run populate-env-vars staging",
    "build:prod": "tsc && npm run populate-env-vars prod",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "npm run build:dev && gcloud app deploy --project=entur-dev",
    "deploy:staging": "npm run build:staging && gcloud app deploy --project=entur-staging",
    "deploy:prod": "npm run build:prod && gcloud app deploy --project=entur-prod",
    "populate-env-vars": "ts-node ./populateEnvVars.ts"
  },
  "dependencies": {
    "@entur/sdk": "^1.0.0-next.42",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "express": "^4.17.1",
    "lz-string": "^1.4.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/lz-string": "^1.3.33",
    "@types/node": "^13.1.6",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.4"
  }
}
