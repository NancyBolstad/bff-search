  "scripts": {
    "start": "node .",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "nodemon --watch src -e ts,js,json --exec 'npm run build:dev && node' .",
    "staging": "nodemon --watch src -e ts,js,json --exec 'npm run build:staging && node' .",
    "prod": "nodemon --watch src -e ts,js,json --exec 'npm run build:prod && node' .",
    "build:dev": "tsc && npm run populate-env-vars dev",
    "build:staging": "tsc && npm run populate-env-vars staging",
    "build:prod": "tsc && npm run populate-env-vars prod",
    "deploy": "./scripts/deploy.sh",
    "apigee": "./scripts/deploy-apigee.sh",
    "populate-env-vars": "ts-node ./populateEnvVars.ts"
  },
  "dependencies": {
    "@entur/sdk": "^1.4.0",
    "@google-cloud/bigquery": "^4.7.0",
    "@google-cloud/logging-winston": "^3.0.5",
