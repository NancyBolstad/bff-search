{
  "name": "@entur/bff-search",
  "version": "1.0.0",
  "description": "A backend that handles travel search for Entur clients.",
  "main": "dist/server.js",
  "author": "Entur AS",
  "license": "EUPL-1.2",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node .",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "nodemon --watch src -e ts,js,json --exec 'npm run build:dev && node' .",
    "staging": "nodemon --watch src -e ts,js,json --exec 'npm run build:staging && node' .",
    "prod": "nodemon --watch src -e ts,js,json --exec 'npm run build:prod && node' .",
    "build:dev": "tsc && npm run populate-env-vars dev",
    "build:staging": "tsc && npm run populate-env-vars staging",
    "build:prod": "tsc && npm run populate-env-vars prod",
    "deploy": "./scripts/deploy.sh",
    "apigee": "./scripts/deploy-apigee.sh",
    "populate-env-vars": "ts-node ./populateEnvVars.ts"
  },
  "dependencies": {
    "@entur/sdk": "^1.5.2",
    "@google-cloud/bigquery": "^4.7.0",
    "@google-cloud/logging-winston": "^3.0.6",
    "@google-cloud/trace-agent": "^4.2.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.12.0",
    "date-fns-timezone": "^0.1.4",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "jwks-rsa": "^1.8.0",
    "lz-string": "^1.4.4",
    "redis": "^3.0.2",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^13.13.4",
    "@types/redis": "^2.8.18",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "apigeetool": "^0.14.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^24.9.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.9.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.8.3"
  }
}
